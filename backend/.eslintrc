{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": ["warn", { "allow": ["log", "warn", "error"] }],
    "linebreak-style": "off"
  },
  "ignorePatterns": ["**/.mjs"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "requireConfigFile": false
  }
}
